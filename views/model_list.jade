include ./model_nav

doctype html
html
  head
    title= title
    link(rel='stylesheet', href='/stylesheets/foundation.css')
    script(type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.4/jquery.min.js")
    script(src="/javascripts/jquery.form.min.js")
    script(type="text/javascript" src="/javascripts/foundation.min.js")
    script(type="text/javascript" src="/javascripts/init.js")
    
  body
    +nav_topbar(isLogin)

    h1(style!="text-align:center;") Database Models

    - each obj in objs
        div(class!="large-4 medium-6 columns end")
          div(class="callout panel")
            img(
              src=obj.get('objThumbnail').url()
              style="display:block; margin:0 auto; max-height:250px; min-height:250px; max-width:100%;"
            )
            p(style="text-align:center")= obj.get('name')
        
